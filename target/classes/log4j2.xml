<?xml version="1.0" encoding="UTF-8"?>
<!-- 0809 -->
<Configuration status="INFO">
    <Appenders>
        <Console name="ConsoleAppender" target="SYSTEM_OUT"><!-- [%C] %-5level %logger{36} - %msg -->
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" /> <!-- 0805 로그 너무 길어서 저거 주석처리함 -->
        </Console>
   
 
    </Appenders>
    
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="ConsoleAppender" />
        </Root>
    </Loggers>
</Configuration>